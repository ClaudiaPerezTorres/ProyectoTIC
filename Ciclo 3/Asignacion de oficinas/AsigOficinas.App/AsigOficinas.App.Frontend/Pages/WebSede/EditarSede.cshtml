@page
@model AsigOficinas.App.Frontend.Pages.EditarModel
@{
}
<h2>Editar sede</h2>
<form method="post">
    <input type="hidden" asp-for="sede.id">
    <div class="form-group">
        <label asp-for="sede.nombre"></label>
        <input asp-for="sede.nombre" class="form-control">
        <span class="text-danger" asp-validation-for="sede.nombre"></span>
    </div>
    <div class="form-group">
        <label asp-for="sede.ubicacion"></label>
        <input asp-for="sede.ubicacion" class="form-control">
        <span class="text-danger" asp-validation-for="sede.ubicacion"></span>
    </div>
    <button type="submit" class="btn btn-primary">Guardar</button>
</form>