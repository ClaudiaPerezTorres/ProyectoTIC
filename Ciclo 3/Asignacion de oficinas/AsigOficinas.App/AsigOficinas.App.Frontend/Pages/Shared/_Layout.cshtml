<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <meta name="description" content="">
    <meta name="author" content="Joan Cortes">
    <meta name="generator" content="Los Ppwer Rangers">
    <title>@ViewData["Title"] - Asignación de oficinas</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    @* Uso de Bootstrap *@
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.1.2/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-uWxY/CJNBR+1zjPWmfnSnVxwRheevXITnMqoEIeG1LJrdI0GlVs/9cVSyPYXdcSF" crossorigin="anonymous">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.5.0/font/bootstrap-icons.css">
    <!-- Bootstrap core CSS -->
    <link href="~/assets/dist/css/bootstrap.min.css" rel="stylesheet">
</head>

<body>
    <div class="container-fluid">
        <header class="d-flex flex-wrap justify-content-center py-3 mb-4 border-bottom">
            <a href="/" class="d-flex align-items-center mb-3 mb-md-0 me-md-auto text-dark text-decoration-none">
                <i class="fa fa-users fa-3x" aria-hidden="true"></i>
                <span class="fs-4">Asignación de oficinas</span>
            </a>

            <ul class="nav nav-pills">
                <li class="nav-item"><a class="nav-link active" aria-current="page" asp-page="/Index">Inicio</a></li>
                <li class="nav-item"><a class="nav-link" asp-page="/WebGobernacion/gobernacion">Gobernación</a></li>
                <li class="nav-item"><a class="nav-link" asp-page="/WebSede/sede">Sede</a></li>
                <li class="nav-item"><a class="nav-link" asp-page="/WebOficina/Oficina">Oficina</a></li>
                <li class="nav-item dropdown">
                    <a class="nav-link" id="dropdown01" data-bs-toggle="dropdown" aria-expanded="false">Empleados</a>
                    <ul class="dropdown-menu" aria-labelledby="dropdown01">
                        <li><a class="dropdown-item" asp-page="/WebPersonalAseo/personalAseo">Personal de aseo</a></li>
                        <li><a class="dropdown-item" asp-page="/WebProveedorDeServicios/proveedorServicios">Proveedor de servicios</a></li>
                        <li><a class="dropdown-item" asp-page="/WebSecretarioDeDespacho/secretarioDespacho">Secretrario de despachos</a></li>
                        <li><a class="dropdown-item" asp-page="/WebGobernadorYAsesor/gobenadorAsesor">Gobernador y Asesor</a></li>
                        <li><a class="dropdown-item" asp-page="/WebDiagnostico/diagnostico">Diagnostico</a></li>
                    </ul>
                </li>
            </ul>
    </div>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container-fluid">
            &copy; 2021 - Asignación de oficinas - <a asp-area="" asp-page="/Privacy">Privacy</a>
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @* Usar script de Bootstrap *@
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/assets/dist/js/bootstrap.bundle.min.js"></script>
    @* Usar iconos de Fontawesome *@
    <script src="https://use.fontawesome.com/d8887d349b.js"></script>
    <script src="https://kit.fontawesome.com/a076d05399.js" crossorigin="anonymous"></script>
    @RenderSection("Scripts", required: false)
</body>

</html>
