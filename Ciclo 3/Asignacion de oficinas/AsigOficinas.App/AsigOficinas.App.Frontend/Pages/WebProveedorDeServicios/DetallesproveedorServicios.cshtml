@page
@model AsigOficinas.App.Frontend.Pages.DetallesproveedorServiciosModel
@{
}
<h2>Detalles del proveedor de servicios</h2>
<table class="table">
    <thead>
        <tr>
            <th>Nombre</th>
            <th>Apellido</th>
            <th>Identificación</th>
            <th>Edad</th>
            <th>Estado COVID</th>
            <th>Diagnostico</th>
            <th>Servicio realizado</th>
            <th>Und. Desarrollo servicio</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.proveedorDeServicios.nombre</td>
            <td>@Model.proveedorDeServicios.apellido</td>
            <td>@Model.proveedorDeServicios.identificacion</td>
            <td>@Model.proveedorDeServicios.edad</td>
            <td>@Model.proveedorDeServicios.estadoCovid</td>
            <td>@Model.proveedorDeServicios.diagnostico</td>
            <td>@Model.proveedorDeServicios.servicioRealizado</td>
            <td>@Model.proveedorDeServicios.undDesarrolloServicio</td>
        </tr>
    </tbody>
</table>
<a class="btn btn-primary" asp-area="" asp-page="/WebProveedorDeServicios/proveedorServicios"><i
        class="bi bi-arrow-left"></i></i> proveedores de servicios</a>