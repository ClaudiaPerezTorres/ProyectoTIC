@page
@model AsigOficinas.App.Frontend.Pages.proveedorServiciosModel
@{
}
<h1>Listado de proveedorServicios</h1>
<table class="table">
    @foreach (var proveedorDeServicios in Model.proveedoresDeServicios)
    {
        <tr>
            <td>@proveedorDeServicios.nombre</td>
            <td>@proveedorDeServicios.apellido</td>
            <td>@proveedorDeServicios.identificacion</td>
            <td>@proveedorDeServicios.edad</td>
            <td>@proveedorDeServicios.estadoCovid</td>
            <td>@proveedorDeServicios.diagnostico</td>
            <td>@proveedorDeServicios.servicioRealizado</td>
            <td>@proveedorDeServicios.undDesarrolloServicio</td>
            <td>
                <a class="btn btn-primary table-btn" asp-page="./DetallesproveedorServicios" asp-route-idProveedorDeServicios="@proveedorDeServicios.id">
                    <i class="bi bi-info-circle" aria-hidden="true"></i>
                    Detalle
                </a>
            </td>
            <td>
                <a class="btn btn-secondary table-btn" asp-page="./EditarproveedorServicios" asp-route-idProveedorDeServicios="@proveedorDeServicios.id">
                    <i class="bi bi-pencil-square" aria-hidden="true"></i>
                    Editar
                </a>
            </td>
            <td>
                <a class="btn btn-danger table-btn" asp-page="./EliminarproveedorServicios" asp-route-idProveedorDeServicios="@proveedorDeServicios.id">
                    <i class="bi bi-trash" aria-hidden="true"></i>
                    Eliminar
                </a>
            </td>
        </tr>
    }
</table>
<a class="btn btn-primary table-btn" asp-page="./EditarproveedorServicios">
    <i class="bi bi-file-plus" aria-hidden="true"></i>
    Crear personal de aseo
</a>