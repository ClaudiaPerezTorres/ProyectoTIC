@page
@model AsigOficinas.App.Frontend.Pages.EliminarSistemaModel
@{
}
<h2>Desea eliminar sistema</h2>
<table class="table">
    <thead>
        <tr>
            <th>Oficinas</th>
        </tr>
    </thead>
    <tbody>
        <tr>
            <td>@Model.sistema.oficina</td>
        </tr>
    </tbody>
</table>
<form method="post">
    <input type="hidden" asp-for="sistema.id" />
    <button type="submit" class="btn btn-danger">
        <i class="bi bi-trash" aria-hidden="true"></i>
        Confirmar eliminar
    </button>
</form>